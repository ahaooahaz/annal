{
    "plugins": [
        {
            "name": "zcomet",
            "repo": "agkozak/zcomet",
            "links": [
                {
                    "dst": "plugins/zcomet",
                    "link": "~/.zcomet"
                }
            ]
        },
        {
            "name": "jumpto",
            "repo": "ahaooahaz/jumpto",
            "links": [
                {
                    "dst": "plugins/jumpto/jt.sh",
                    "link": "~/.local/bin/jt"
                }
            ]
        },
        {
            "name": "tmux",
            "repo": "gpakosz/.tmux",
            "links": [
                {
                    "dst": "plugins/tmux",
                    "link": "~/.tmux"
                },
                {
                    "dst": "plugins/tmux/.tmux.conf",
                    "link": "~/.tmux.conf"
                },
                {
                    "dst": "configs/tmux/.tmux.conf.local",
                    "link": "~/.tmux.conf.local"
                }
            ]
        },
        {
            "name": "kitty",
            "links": [
                {
                    "dst": "configs/kitty",
                    "link": "~/.config/kitty"
                }
            ]
        },
        {
            "name": "vim",
            "links": [
                {
                    "dst": "configs/rcs/.vimrc",
                    "link": "~/.vimrc"
                },
                {
                    "dst": "configs/nvim",
                    "link": "~/.config/nvim"
                }
            ]
        },
        {
            "name": "shell",
            "links": [
                {
                    "dst": "configs/rcs/.shrc",
                    "link": "~/.zshrc"
                },
                {
                    "dst": "configs/rcs/.loginrc",
                    "link": "~/.zshenv"
                },
                {
                    "dst": "configs/rcs/.shrc",
                    "link": "~/.bashrc"
                },
                {
                    "dst": "configs/rcs/.loginrc",
                    "link": "~/.profile"
                },
                {
                    "dst": "configs/rcs/.logoutrc",
                    "link": "~/.zlogout"
                },
                {
                    "dst": "configs/rcs/.logoutrc",
                    "link": "~/.bash_logout"
                }
            ]
        },
        {
            "name": "ssh",
            "links": [
                {
                    "dst": "configs/ssh/linux/config",
                    "link": "~/.ssh/config"
                }
            ]
        },
        {
            "name": "scripts",
            "links": [
                {
                    "dst": "scripts/linux",
                    "link": "~/.local/scripts"
                }
            ]
        }
    ]
}
